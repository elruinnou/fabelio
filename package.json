    "test:watch": "jest --config=jest.config.js --watch",
    "test:coverage": "jest --config=jest.config.js --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://erwinardiansyah@bitbucket.org/erwinardiansyah/fabelio.git"
  },
  "keywords": [
    "react",
    "typescript",
    "javascript",
    "jest",
    "boilerplate",
    "rkoziol"
  ],
  "author": "ardiansyaherwin",
  "license": "ISC",
  "bugs": {
    "url": "https://erwinardiansyah@bitbucket.org/erwinardiansyah/fabelio/issues"
  },
  "homepage": "https://erwinardiansyah@bitbucket.org/erwinardiansyah/fabelio#readme",
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint",
      "pre-push": "yarn run test"
